apiVersion: apps/v1
kind: Deployment
metadata:
  name: kubelab-app-staging
  namespace: kubelab-staging
  labels:
    app: kubelab
spec:
  replicas: 3
  selector:
    matchLabels:
      app: kubelab
  template:
    metadata:
      labels:
        app: kubelab
    spec:
      containers:
      - name: kubelab-container
        image: timlinsenmayer/kubelab-insights:latest
        ports:
        - containerPort: 3000
